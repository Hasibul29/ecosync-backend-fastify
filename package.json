{
  "name": "ecosync",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "check:secret": "ts-node src/checkrequired.ts",
    "dev": "npm run build:ts && npm run check:secret && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.0",
    "@fastify/secure-session": "^7.5.1",
    "@fastify/sensible": "^5.0.0",
    "@prisma/client": "^5.12.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.9.0",
    "fastify-plugin": "^4.0.0",
    "nodemailer": "^6.9.13",
    "prisma": "^5.12.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.4.4",
    "@types/nodemailer": "^6.4.14",
    "@types/tap": "^15.0.5",
    "concurrently": "^8.2.2",
    "fastify-tsconfig": "^2.0.0",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2"
  }
}
